<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.pavelsikun.seekbarpreference.SeekBarPreferenceCompat
        android:defaultValue="10"
        android:key="com.techmedia.timer.timer_length"
        android:summary="How long should the timer run?"
        android:title="Session Length"
        app:msbp_dialogEnabled="true"
        app:msbp_interval="1"
